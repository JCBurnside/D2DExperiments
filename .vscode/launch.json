    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "run D2DExperiments",
            "program": "${workspaceFolder}\\target\\debug\\D2DExperiments.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "sourceFileMap": {},
            "preLaunchTask": {
                "type": "cargo",
                "command": "build",
                "problemMatcher": [
                    "$rustc"
                ],
                "group": "build",
                "label": "rust: cargo build"
            }
        }
    ]
}